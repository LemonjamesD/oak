link!("std/time.o");

let ns: struct
{
    // Hidden internals via C++ linking
    __a, __b, __c, __d: u128,
}

let New(self: ^ns) -> void;

let Copy(self: ^ns, other: ns) -> void;
let now(self: ^ns) -> void;
let count(start: ^ns, end: ^ns) -> i128;
