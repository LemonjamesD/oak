let arr<t>: struct
{
    size: i128,
    data: ^t,
}

let New<t>(self: ^arr<t>) -> void
{
    self.size = to_i128(0);
}

let Init<t>(self: ^arr<t>, size: i128) -> void
{
    self.size = size;
    alloc!(self.data, size);
}

let Del<t>(self: ^arr<t>) -> void
{
    free_arr!(self.data);
}

let Get<t>(self: ^arr<t>, index: i128) -> ^t
{
    Get(@self.data, index)
}
