package!("std");
include!("extra/bit_print.oak");
include!("extra/bitset.oak");

use_rule!("std");

let println(what: bool) -> void
{
    if what
    {
        print("true\n");
    }
    else
    {
        print("false\n");
    }
}

let main() -> i32
{
    bitset<64>;

    let b: bitset<64>;

    let c = b.Get(to_u128(0));
    let res = c.unwrap();
    println(res);

    b.flip(to_u128(0));

    c = b.Get(to_u128(0));
    res = c.unwrap();
    println(res);

    b.clear(to_u128(0));

    0
}
