// For testing Oak's generic structs

package!("std");
use_rule!("std");

// Linked list node
let node<t>: struct
{
    data: t,
    next: ^node<t>,
}

let AddEq<t>(self: ^node<t>, data: t) -> void
{
    alloc!(self.next);
    self.next.data = data;
}

let Del<t>(self: ^node<t>) -> void
{
    free!(self.next);
}

let main() -> i32
{
    node<i32>;
    AddEq<i32>;
    Del<i32>;

    // Create a node
    let head: node<i32>;
    head.data = 123;

    Del(@head);

    0
}
