package!("std");

// Test of generics; Compiler should instantiate this without using C++ templates
let println<t>(what: t) -> void
{
    print(what);
    print("\n");
}

let main(argc: i32, argv: ^str) -> i32
{
    print("Hello, world!\n");

    println<i32>;
    println<str>;

    println("Hiiii");

    let i: i32;
    i = 5;
    println(i);

    0
}
