package!("std");

let obj: struct
{
    a: i32,
}

let Copy(self: ^obj, what: i32)
{
    self.a = what;
}

let method(self: ^obj)
{
    print(self.a);
    print("\n");
}

let main() -> i32
{
    print("Hello, there!\n");

    let inst: obj = 5;
    inst.method();

    0
}
