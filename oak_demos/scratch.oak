package!("std");
use_rule!("std");

let test: struct
{
    a, b, c: i32,
}

let New(self: ^test) -> void
{
    self.a = 0;
    self.b = 0;
    self.c = 0;
}

let Copy(self: ^test, a: i32, b: i32, c: i32) -> void
{
    self.a = a;
    self.b = b;
    self.c = c;
}

let main() -> i32
{
    let obj: test;
    obj = (1, 2, 3);

    0
}
