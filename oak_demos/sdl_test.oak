package!("std");
package!("sdl");

let main() -> i32
{
    print("hi\n");

    sdl_init();

    let wind: sdl_window;

    let black: sdl_color;
    let white: sdl_color;
    New(@white, i32_to_u8(255), i32_to_u8(255), i32_to_u8(255), i32_to_u8(255));

    fill(@wind, black);

    let rect: sdl_rect;
    New(@rect, i32_to_u64(10), i32_to_u64(10), i32_to_u64(10), i32_to_u64(10));
    draw_rect(@wind, rect, white);
    
    show(@wind);   

    sdl_delay(1000);

    Del(@wind);

    sdl_quit();

    0
}
