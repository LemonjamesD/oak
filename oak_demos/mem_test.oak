package!("std");
use_rule!("std_method");

let test: struct
{
    data: ^i32,
}

let New(self: ^test) -> void
{
    print("Alloc called\n");

    alloc!(self.data, 5);
}

let Del(self: ^test) -> void
{
    print("Free called\n");

    free_arr!(self.data);
}

let main() -> i32
{
    let a: test;

    a..Del();

    0
}